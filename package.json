{
    "name": "currencyApp",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prebuild": "rm -fr ./dist/* && mkdir -p ./dist",
        "build": "tsc ./src/*.ts --outDir ./dist --sourceMap --target es5 --lib es5,es6,dom,scripthost",
        "postbuild": "paeckchen -e ./dist/main.js -f ./dist/main.js --sourceMap",
        "prebuild:test": "rm -fr ./test/dist/* && mkdir -p ./test/dist",
        "build:test": "tsc ./test/*.ts --outDir ./test/dist/ --sourceMap --module commonjs --target es5 --noImplicitAny --moduleResolution node --lib es5,es6,dom,scripthost",
        "pretest": "npm run build:test",
        "test": "mocha ./test/dist/test/index.js --verbose",
        "posttest": "tslint -c ./tslint.json './{src,test}/**/*.ts'",
        "styleguide-server": "styleguidist server",
        "styleguide-build": "styleguidist build"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "chai": "^3.5.0",
        "mocha": "^3.0.2",
        "paeckchen-cli": "^0.4.0",
        "react": "^15.3.2",
        "react-dom": "^15.3.2",
        "react-styleguidist": "^4.1.0",
        "readline": "^1.3.0",
        "sinon": "^1.17.6",
        "tslint": "^3.15.1",
        "typescript": "^2.0.0",
        "typings": "^1.3.3",
        "webpack": "^1.13.2",
        "xmlhttprequest": "^1.8.0"
    },
    "dependencies": {
        "currstyles": "file:../currStyles",
        "react": "^15.3.2",
        "react-dom": "^15.3.2"
    }
}
